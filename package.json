{
  "name": "nuxt-app",
  "private": true,
  "type": "module",
  "engines": {
    "node": ">=20.0.0"
  },
  "scripts": {
    "build": "nuxt build",
    "dev": "nuxt dev",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "lint": "eslint",
    "lint:fix": "eslint . --fix",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "validate": "tsc && npm run lint && npm run test"
  },
  "dependencies": {
    "@nuxt/eslint": "^0.5.2",
    "@nuxt/fonts": "^0.7.2",
    "@nuxt/icon": "^1.4.8",
    "@nuxt/image": "^1.7.1",
    "@nuxtjs/i18n": "^8.5.0",
    "@nuxtjs/tailwindcss": "^6.12.1",
    "@pinia/nuxt": "^0.5.4",
    "@rushstack/eslint-patch": "^1.10.4",
    "nuxt": "^3.13.0",
    "pinia": "^2.2.2",
    "vue": "3.4.38"
  },
  "prettier": {
    "semi": false,
    "singleQuote": true,
    "trailingComma": "none",
    "tabWidth": 2,
    "printWidth": 100,
    "bracketSpacing": true,
    "arrowParens": "avoid",
    "endOfLine": "auto",
    "vueIndentScriptAndStyle": true
  },
  "devDependencies": {
    "@iconify-json/flagpack": "^1.1.15",
    "@nuxt/test-utils": "^3.14.1",
    "@nuxtjs/eslint-config": "^12.0.0",
    "@pinia-plugin-persistedstate/nuxt": "^1.2.1",
    "@playwright/test": "^1.46.1",
    "@vue/test-utils": "^2.4.6",
    "eslint": "^8.57.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-vue": "^9.27.0",
    "happy-dom": "^14.12.3",
    "playwright-core": "^1.46.1",
    "prettier": "3.3.3",
    "vitest": "^2.0.5"
  }
}
